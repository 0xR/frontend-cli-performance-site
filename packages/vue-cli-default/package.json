{
  "name": "vue-cli-default",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "http-server dist",
    "dev": "npm run serve",
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lighthouse": "lighthouse https://vue-cli-default-dot-ruben-oostinga-speeltuin.appspot.com/ --output json --output-path report.json"
  },
  "dependencies": {
    "http-server": "0.11.1",
    "lighthouse": "3.0.3",
    "vue": "2.5.16"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.0.0-rc.7",
    "@vue/cli-plugin-eslint": "3.0.0-rc.7",
    "@vue/cli-service": "3.0.0-rc.7",
    "vue-template-compiler": "2.5.16"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
